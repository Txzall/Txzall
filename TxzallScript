---------------------------------------------------
-- UI Elements: Toggles, Sliders, and Inputs
---------------------------------------------------

-- Noclip Toggle (Safe with respawn re-enable)
local noclipConnection
local reEnableNoclip = false

scriptsSection:Toggle({
    Title = "Noclip Toggle",
    Desc = "Turns noclip on and off (auto re-enable on respawn)",
    Default = false,
    Callback = function(state)
        local player = game.Players.LocalPlayer
        local character = player.Character
        if not character then return end
        
        -- Save state biar kalau respawn tetep nyala
        reEnableNoclip = state

        if state then
            noclipConnection = game:GetService("RunService").Stepped:Connect(function()
                for _, part in ipairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        else
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
                for _, part in ipairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
        end
    end
})

-- Auto apply noclip again if respawn
game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
    if reEnableNoclip then
        task.wait(1) -- biar character kebentuk dulu
        noclipConnection = game:GetService("RunService").Stepped:Connect(function()
            for _, part in ipairs(char:GetChildren()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)
    end
end)

